#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int a,b,c;
    scanf("%d%d%d",&a,&b,&c);
    int r1=(b*75)+(c*30);
    if(r1<=a)
    {
        printf("Boat is stable");
    }
    else{
        printf("Boat will drown");
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
