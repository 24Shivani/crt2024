#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
char str1[50];
    scanf("%[^\n]s",str1);
int i,dig=0;

for(i=0;str1[i]!='\0';i++){

 if(str1[i]>=48&&str1[i]<=57){
    dig=dig+str1[i]-48;

    }
}
    

printf("%d",dig);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
